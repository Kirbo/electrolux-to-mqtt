{
  "name": "electrolux-to-mqtt",
  "version": "development",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "check": "pnpm biome check --write",
    "clean": "rimraf node_modules",
    "dev:docker": "NODE_VERSION=$(cat .nvmrc) docker compose -f docker-compose.local.yml down && NODE_VERSION=$(cat .nvmrc) docker compose -f docker-compose.local.yml up --build",
    "dev": "tsx watch src/index.ts",
    "format": "pnpm biome format --write",
    "lint": "pnpm biome lint --write",
    "nuke": "pnpm clean ; pnpm install",
    "postinstall": "[ -f /.dockerenv ] || [ -n \"$CI\" ] || [ -n \"$DOCKER_BUILD\" ] && exit 0; bash postinstall.sh || exit 0",
    "start": "tsx src/index.ts",
    "test": "vitest run --coverage",
    "test:e2e": "E2E_TEST=true pnpm test tests/e2e/electrolux-api.e2e.test.ts",
    "test:watch": "vitest",
    "sonar": "pnpm test && sonar-scanner",
    "update-deps": "pnpm update"
  },
  "keywords": [],
  "author": "Kimmo Saari <kirbo@kirbo-designs.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.28.0",
  "dependencies": {
    "axios": "^1.13.2",
    "mqtt": "^5.14.1",
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3",
    "tiny-lru": "^11.4.7",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^4.0.17",
    "tsx": "^4.21.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild"
    ]
  }
}
